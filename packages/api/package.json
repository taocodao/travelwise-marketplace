{
  "name": "@travelwise/api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "dev": "tsx watch src/index.ts",
    "test:ai-agent": "tsx src/test-ai-agent.ts",
    "mcp:google-maps": "tsx src/mcp-servers/google-maps/index.ts",
    "mcp:weather": "tsx src/mcp-servers/weather/index.ts",
    "mcp:travel-agent": "tsx src/mcp-servers/travel-agent/index.ts",
    "mcp:all": "concurrently \"npm:mcp:*\"",
    "start:all": "concurrently \"npm:dev\" \"npm:mcp:all\""
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ethers": "^6.9.2",
    "express": "^4.22.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "openai": "^6.10.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.2",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
